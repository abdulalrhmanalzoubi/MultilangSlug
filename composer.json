{
    "name": "abdulrahman/multilang-slug",
    "description": "A package to generate multilingual slugs easily for Laravel models",
        "keywords": ["laravel", "slug", "multilingual", "slugify"],

    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Abdulrahman\\MultilangSlug\\": "src/"
        }
    },
    "require": {
        "php": "^8.1",
        "illuminate/support": "^12.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Abdulrahman\\MultilangSlug\\MultilangSlugServiceProvider"
            ]
        }
    }
}
